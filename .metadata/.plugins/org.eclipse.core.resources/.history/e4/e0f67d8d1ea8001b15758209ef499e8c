package com.list;

import java.util.ArrayList;

public class ReadDataFromList {

	public static void main(String[] args) {
		
		ArrayList al= new ArrayList();
		
		al.add(1);
		al.add('A');
		al.add("hello");
		al.add(10.5);
		al.add(true);
		al.add(10);
		
		System.out.println("List of elements in AL is"+al);
		System.out.println(al.size());
	}

}
